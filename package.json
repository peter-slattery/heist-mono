{
  "name": "heist-mono",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "plugins/",
    "app/"
  ],
  "scripts": {
    "test": "jest",
    "db:down": "docker compose -f backend/infra/local-dynamodb.compose.yml down",
    "db:up": "docker compose -f backend/infra/local-dynamodb.compose.yml up",
    "dev:vite": "npm run dev --workspace=app",
    "dev": "netlify dev",
    "run_migrations": "ts-node --skip-project ./backend/infra/migrations/run.ts",
    "build:vite": "npm run build --workspace=app",
    "build:infra": "npm run run_migrations",
    "build": "run-s build:*",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.405.0",
    "@netlify/functions": "^2.0.2",
    "@types/node": "^20.5.9",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "nanoid": "^4.0.2",
    "netlify-identity-widget": "^1.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jss": "^10.10.0",
    "react-router-dom": "^6.15.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/netlify-identity-widget": "^1.9.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest": "^29.6.4",
    "netlify-cli": "^16.2.0",
    "npm": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
